{
    "name": "drewlabs/http-cookie",
    "description": "HTTP cookie implementation library",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "http",
        "cookies"
    ],
    "autoload": {
        "psr-4": {
            "Drewlabs\\Cookies\\": "src/"
        }
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "azandrewdevelopper@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1|^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0"
    }
}
